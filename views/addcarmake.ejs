<% include partials/header %>
<div class="container">
<div class="row">
    <div class="col-lg-4"> 
        <form action="/addcarmake" method="POST">
            <div class="form-group">
                <input type="text" class="form-control" placeholder="car make" name="make"></input>
        
            </div>
            <button class="btn btn-primary">Save</button>
        </form>
    </div>
    <div class="col-lg-4"> 
        <form action="/addcarmodel" method="POST">
            <select class="form-control my-2" id="carmake" name="carmake">
                <% carmake.forEach(function(carmake){ %>
                <option><%= carmake.make %></option>
                <% }); %>
              </select>
    
            <div class="form-group">
                <input type="text" class="form-control" placeholder="car model" name="model"></input>
        
            </div>
            <button class="btn btn-primary">Save</button>
    </form>
    </div>
    <div class="col-lg-4"> 
        <form id="displaymodel"> 
            <select class="form-control my-2" id="dcarmake" name="carmake">
                    <% carmake.forEach(function(carmake){ %>
                    <option><%= carmake.make %></option>
                    <% }); %>
                  </select>
       
        
        <button class="btn btn-primary">Display</button>
    
        <div id="display" class="container">
        
        </div>
    </form>
    </div>
</div>
</div>

    




    
  <% include partials/footer %>